  "scripts": {
    "prepare": "husky",
    "format": "nx format:write --all",
    "format:check": "nx format:check --all",
    "lint": "nx run-many -t lint --all",
    "typecheck": "nx run-many -t typecheck --all",
    "build": "nx run-many -t build --all",
    "test": "vitest run",
    "dev:api": "nx serve api",
    "dev:web": "nx dev web",
